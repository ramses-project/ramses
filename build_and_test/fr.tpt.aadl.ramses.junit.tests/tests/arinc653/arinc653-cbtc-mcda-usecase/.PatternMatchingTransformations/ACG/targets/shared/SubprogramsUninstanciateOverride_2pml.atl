-- @extends m_Component_Type
rule m_Subprogram_Type {
 from
  type : AADLI!SubprogramType
 to
  t : MM_ATL2PM!ElementTransformationTuple (
   key <:= Sequence {type},
   value <- 'SubprogramsUninstanciateOverride.m_Subprogram_Type'
  )
