package Dummy::Types

public
  with Data_Model ;

  data Opaque
  end Opaque;
  
  data implementation Opaque.v1
  end Opaque.v1 ;
  
  data Integer
    properties
      Data_Model::Data_Representation => Integer;
      Data_Model::Number_Representation => Signed;
  end Integer ;
  
  data implementation Integer.v1
  end Integer.v1 ;
  
  data Days
    properties
    Data_Model::Data_Representation => Enum;
    Data_Model::Enumerators => ("unknown", "monday", "thuesday", "wednesday", "thrusday",
                              "friday", "saturday", "sunday");
  end Days ;
  
  data A_struct
    properties
      Data_Model::Data_Representation => Struct;
      Data_Model::Base_Type => (classifier(Days),classifier(Days));
      Data_Model::Element_Names => ("a","b");
  end A_struct;

  data A_union
    properties
      Data_Model::Data_Representation => Union;
      Data_Model::Base_Type => (classifier(Days),classifier(Days));
      Data_Model::Element_Names => ("a","b");
  end A_union;

  data implementation Days.v1
    properties
    Data_Model::Representation => ("7","0","1","2","3","4","5","6");
  
  end Days.v1 ;

  data implementation Days.v2
    properties
    Data_Model::Representation => ("8","1","2","3","4","5","6","7");
  
  end Days.v2 ;  
  
end Dummy::Types ;